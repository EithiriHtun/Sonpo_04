/* ------------------------------------------
	検索結果ページ用指定
--------------------------------------------- */
#searchresult #moresearch{
}

#searchresult #moresearch #tit{
	float:left;
	width:7.5em;
	margin:3px 0px 0 0px;
}

#searchresult #moresearch #tit1{
	float:left;
	margin:3px 5px 0px 0px;
}

